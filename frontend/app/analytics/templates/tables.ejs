<div class="analytics-tables">
  <form class="well analytics-controls analytics-tables-controls form-inline">
    <label for="<%- idPrefix %>-controls-tag"><%- t('analytics', 'CHART_CONTROLS_TAG') %></label>
    <select id="<%- idPrefix %>-controls-tag" class="is-expandable" multiple size="1" data-expanded-length="22" required>
      <optgroup label="<%- t('tags', 'GROUP:inputPumps') %>">
        <option value="inputPumps.1.current"><%- t('tags', 'TAG:inputPumps.1.current') %></option>
        <option value="inputPumps.1.waterLevel"><%- t('tags', 'TAG:inputPumps.1.waterLevel') %></option>
        <option value="inputPumps.2.current"><%- t('tags', 'TAG:inputPumps.2.current') %></option>
        <option value="inputPumps.2.waterLevel"><%- t('tags', 'TAG:inputPumps.2.waterLevel') %></option>
      </optgroup>
      <optgroup label="<%- t('tags', 'GROUP:outputPumps') %>">
        <option value="outputPumps.current"><%- t('tags', 'TAG:outputPumps.current') %></option>
        <option value="outputPumps.outputFrequency"><%- t('tags', 'TAG:outputPumps.outputFrequency') %></option>
        <option value="outputPumps.presetRef"><%- t('tags', 'TAG:outputPumps.presetRef') %></option>
      </optgroup>
      <optgroup label="<%- t('tags', 'GROUP:reservoirs') %>">
        <option value="reservoirs.1.waterLevel"><%- t('tags', 'TAG:reservoirs.1.waterLevel') %></option>
        <option value="reservoirs.2.waterLevel"><%- t('tags', 'TAG:reservoirs.2.waterLevel') %></option>
      </optgroup>
      <optgroup label="<%- t('tags', 'GROUP:pressureSensors') %>">
        <option value="compressorPressure"><%- t('tags', 'TAG:compressorPressure') %></option>
        <option value="outputPressure"><%- t('tags', 'TAG:outputPressure') %></option>
      </optgroup>
      <optgroup label="<%- t('tags', 'GROUP:flowMeters') %>">
        <option value="inputFlow"><%- t('tags', 'TAG:inputFlow') %></option>
        <option value="inputFlow.total.forwards" data-value-field="max"><%- t('tags', 'TAG:inputFlow.total.forwards') %></option>
        <option value="washingFlow"><%- t('tags', 'TAG:washingFlow') %></option>
        <option value="washingFlow.total.forwards" data-value-field="max"><%- t('tags', 'TAG:washingFlow.total.forwards') %></option>
        <option value="outputFlow"><%- t('tags', 'TAG:outputFlow') %></option>
        <option value="outputFlow.total.forwards" data-value-field="max"><%- t('tags', 'TAG:outputFlow.total.forwards') %></option>
      </optgroup>
    </select>
    <label for="<%- idPrefix %>-controls-from"><%- t('analytics', 'CHART_CONTROLS_FROM') %></label>
    <input id="<%- idPrefix %>-controls-from" name="from" type="date" placeholder="YYYY-MM-DD" value="<%- from %>" required>
    <label for="<%- idPrefix %>-controls-to"><%- t('analytics', 'CHART_CONTROLS_TO') %></label>
    <input id="<%- idPrefix %>-controls-to" name="to" type="date" placeholder="YYYY-MM-DD" value="<%- to %>" required>
    <label for="<%- idPrefix %>-controls-interval"><%- t('analytics', 'CHART_CONTROLS_INTERVAL') %></label>
    <div class="btn-group" data-toggle="buttons-radio">
      <button class="btn" type="button" name="interval" value="monthly">mc</button>
      <button class="btn active" type="button" name="interval" value="daily">dzie≈Ñ</button>
      <button class="btn" type="button" name="interval" value="hourly">godz</button>
    </div>
    <button id="<%- idPrefix %>-controls-submit" class="btn btn-primary" type="submit">
      <i class="icon-table"></i>
      <%- t('analytics', 'TABLE_CONTROLS_SUBMIT') %>
    </button>
  </form>
  <div class="analytics-tables-table"></div>
</div>
