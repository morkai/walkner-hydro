<div class="form-horizontal diag-settings">
  <hr>
  <div class="control-group">
    <label class="control-label"><%- t('diagnostics', 'SETTINGS:outputPumps.workMode') %></label>
    <div class="controls">
      <label class="radio-inline">
        <input type="radio" name="outputPumps.workMode" value="onoff">
        <%- t('diagnostics', 'SETTINGS:outputPumps.workMode:onoff') %>
      </label>
      <label class="radio-inline">
        <input type="radio" name="outputPumps.workMode" value="constant">
        <%- t('diagnostics', 'SETTINGS:outputPumps.workMode:constant') %>
      </label>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-maxConstantRunTime"><%- t('diagnostics', 'SETTINGS:outputPumps.maxConstantRunTime') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-maxConstantRunTime" class="span2" type="number" name="outputPumps.maxConstantRunTime" required min="1" max="10080" step="1">
        <span class="add-on">min</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-minTotalWaterLevel"><%- t('diagnostics', 'SETTINGS:outputPumps.minTotalWaterLevel') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-minTotalWaterLevel" class="span2" type="number" name="outputPumps.minTotalWaterLevel" required min="0" max="100" step="0.1" autofocus>
        <span class="add-on">m</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-minOutputPressure"><%- t('diagnostics', 'SETTINGS:outputPumps.minOutputPressure') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-minOutputPressure" class="span2" type="number" name="outputPumps.minOutputPressure" required min="0" max="10" step="0.1">
        <span class="add-on">bar</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-maxOutputPressure"><%- t('diagnostics', 'SETTINGS:outputPumps.maxOutputPressure') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-maxOutputPressure" class="span2" type="number" name="outputPumps.maxOutputPressure" required min="0" max="10" step="0.1">
        <span class="add-on">bar</span>
      </div>
    </div>
  </div>
  <hr>
  <div class="control-group">
    <label class="control-label diag-settings-header" for="settings-outputPumps-stabilizationTimes-firstPump"><%- t('diagnostics', 'SETTINGS_STABILIZATION_TIMES_HEADER') %></label>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-stabilizationTimes-firstPump"><%- t('diagnostics', 'SETTINGS:outputPumps.stabilizationTimes.firstPump') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-stabilizationTimes-firstPump" class="span2" type="number" name="outputPumps.stabilizationTimes.firstPump" required min="0" max="100" step="1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-stabilizationTimes-nextPump"><%- t('diagnostics', 'SETTINGS:outputPumps.stabilizationTimes.nextPump') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-stabilizationTimes-nextPump" class="span2" type="number" name="outputPumps.stabilizationTimes.nextPump" required min="0" max="100" step="1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-outputPumps-stabilizationTimes-stopPump"><%- t('diagnostics', 'SETTINGS:outputPumps.stabilizationTimes.stopPump') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-stabilizationTimes-stopPump" class="span2" type="number" name="outputPumps.stabilizationTimes.stopPump" required min="0" max="100" step="1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <hr>
  <div class="control-group">
    <label class="control-label diag-settings-header" for="settings-outputPumps-presetRef-startingValue"><%- t('diagnostics', 'SETTINGS_PRESET_REF_HEADER') %></label>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-startingValue"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.startingValue') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-startingValue" class="span2" type="number" name="outputPumps.presetRef.startingValue" required min="0" max="100" step="1">
        <span class="add-on">%</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-adjustDelay"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.adjustDelay') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-adjustDelay" class="span2" type="number" name="outputPumps.presetRef.adjustDelay" required min="0" max="120" step="1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-stepInterval"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.stepInterval') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-stepInterval" class="span2" type="number" name="outputPumps.presetRef.stepInterval" required min="0" max="120" step="1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-stepValue"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.stepValue') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-stepValue" class="span2" type="number" name="outputPumps.presetRef.stepValue" required min="0" max="100" step="1">
        <span class="add-on">%</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-minValue"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.minValue') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-minValue" class="span2" type="number" name="outputPumps.presetRef.minValue" required min="0" max="100" step="1">
        <span class="add-on">%</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="outputPumps-presetRef-maxValue"><%- t('diagnostics', 'SETTINGS:outputPumps.presetRef.maxValue') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-outputPumps-presetRef-maxValue" class="span2" type="number" name="outputPumps.presetRef.maxValue" required min="1" max="100" step="1">
        <span class="add-on">%</span>
      </div>
    </div>
  </div>
  <hr>
</div>
