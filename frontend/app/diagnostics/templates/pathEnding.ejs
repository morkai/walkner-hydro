<linearGradient id="diag-path-ending-<%- dir %><%- type ? ('-' + type) : '' %>" class="el-path-ending<%- type ? (' el-path-ending-' + type) : '' %>"
  <% if (dir === 'lr') { %>
  x1="1" x2="0" y1="0" y2="0"
  <% } else if (dir === 'rl') { %>
  x1="0" x2="1" y1="0" y2="0"
  <% } else if (dir === 'tb') { %>
  x1="0" x2="0" y1="0" y2="1"
  <% } else if (dir === 'bt') { %>
  x1="0" x2="0" y1="1" y2="0"
  <% } %>
>
  <stop offset="0" />
  <stop offset="0.8" />
  <stop offset="1" />
</linearGradient>
