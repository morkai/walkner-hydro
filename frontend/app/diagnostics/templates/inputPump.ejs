<div>
  <div class="well fixed-column-right diag-manual-controls">
    <div class="diag-manual-asset-group">
      <div class="diag-manual-asset" data-tag="inputPumps.<%- index %>.switch">
        <img class="diag-manual-asset-image diag-inputPump-switch-image" src="/assets/img/switch.svg?value=0">
        <span class="diag-manual-asset-label"><%- t('diagnostics', 'INPUT_PUMP_SWITCH') %></span>
      </div><div class="diag-manual-asset" data-tag="inputPumps.<%- index %>.status">
        <img class="diag-manual-asset-image diag-inputPump-status-image" src="/assets/img/led.svg?color=grey">
        <span class="diag-manual-asset-label"><%- t('diagnostics', 'INPUT_PUMP_STATUS') %></span>
      </div>
    </div>
    <div class="diag-manual-asset-group">
      <div class="diag-manual-asset" data-tag="inputPumps.<%- index %>.dryRunLed">
        <img class="diag-manual-asset-image diag-inputPump-dryrun-image" src="/assets/img/led.svg?color=grey" data-on-color="yellow">
        <span class="diag-manual-asset-label"><%- t('diagnostics', 'INPUT_PUMP_DRYRUN') %></span>
      </div><div class="diag-manual-asset" data-tag="inputPumps.<%- index %>.failure">
        <img class="diag-manual-asset-image diag-inputPump-failure-image" src="/assets/img/led.svg?color=grey" data-on-color="red">
        <span class="diag-manual-asset-label"><%- t('diagnostics', 'INPUT_PUMP_FAILURE') %></span>
      </div>
    </div>
    <hr>
    <div class="diag-state-switch make-switch switch-large btn-block"
         data-tag="inputPumps.<%- index %>.state"
         data-text-label="<%- t('diagnostics', 'STATE_SWITCH_LABEL') %>"
         data-on-label="<%- t('diagnostics', 'STATE_SWITCH_LABEL_ON') %>"
         data-off-label="<%- t('diagnostics', 'STATE_SWITCH_LABEL_OFF') %>"
         data-on="success"
         data-off="danger">
      <input type="checkbox">
    </div>
    <div class="diag-mode-switch make-switch switch-large btn-block"
         data-tag="inputPumps.<%- index %>.mode"
         data-text-label="<%- t('diagnostics', 'MODE_SWITCH_LABEL') %>"
         data-on-label="<%- t('diagnostics', 'MODE_SWITCH_LABEL_ON') %>"
         data-off-label="<%- t('diagnostics', 'MODE_SWITCH_LABEL_OFF') %>"
         data-on="primary"
         data-off="warning">
      <input type="checkbox">
    </div>
    <hr>
    <button class="btn btn-large btn-block diag-manual-button diag-control-button" data-tag="inputPumps.<%- index %>.control">
      <img class="diag-manual-button-image diag-inputPump-control-image" src="/assets/img/led.svg?color=grey">
      <span class="diag-manual-button-label"><%- t('diagnostics', 'INPUT_PUMP_CONTROL_BUTTON') %></span>
      <i class="icon-spinner icon-spin"></i>
    </button>
  </div>
  <div class="fluid-column-right diag-el-container">
    <svg class="diag-inputPump" width="100%" height="100%" viewBox="0 0 303 425" xmlns="http://www.w3.org/2000/svg">
      <defs>
        <style>
          .al-standing path {
            fill: none;
            stroke: #000;
          }
          .al-standing .al-standing-limb {
            fill: #000;
          }
        </style>
        <linearGradient id="<%- idPrefix %>-waterLevel-fill" x1="0" x2="0" y1="0" y2="1">
          <stop offset="0" stop-color="#aef"></stop>
          <stop offset="1" stop-color="#acf"></stop>
        </linearGradient>
        <%- renderPathEnding({dir: 'tb', type: null}) %>
        <%- renderPathEnding({dir: 'tb', type: 'rawWater'}) %>
      </defs>

      <g transform="translate(0,60)">
        <g class="el-inputPump-well">
          <g transform="translate(114, 0)">
            <rect class="el-inputPump-well-fill" x="0" y="1" width="75" height="349" fill="url(#<%- idPrefix %>-waterLevel-fill)" />
            <rect class="el-inputPump-well-waterLevel" x="0" y="0" width="75" height="100" data-guide="#<%- idPrefix %>-waterLevel-guide" />
            <g id="<%- idPrefix %>-waterLevel-guide" class="el-guide">
              <path d="M 0 0 h -25" />
              <path class="el-guide-v" d="M -25 0 v -291" />
              <text x="-28" y="-145.5" text-anchor="end">00.00m</text>
            </g>
            <g id="<%- idPrefix %>-offset" transform="translate(0, 20)" data-tag="inputPumps.<%- index %>.depth.offset">
              <path class="el-inputPump-well-offset" d="M 0 0 h 75" />
              <g class="el-guide">
                <path d="M 75 0 h 25" />
                <path class="el-guide-v" d="M 100 0 v -20" />
                <text x="103" y="-7.5">00.00m</text>
              </g>
            </g>
            <g id="<%- idPrefix %>-sensor" class="el-inputPump-sensor" transform="translate(75, 291)" data-tag="inputPumps.<%- index %>.depth.sensor">
              <path d="M -1 -5 a 5,5 0 1,0 0,10" />
              <g class="el-guide">
                <path d="M 0 0 h 75" />
                <path class="el-guide-v" d="M 75 0 v -291" />
                <text x="78" y="-145.5">00.00m</text>
              </g>
            </g>
            <g id="<%- idPrefix %>-pump" transform="translate(37.5, 326)" data-tag="inputPumps.<%- index %>.depth">
              <g class="el-guide">
                <path d="M -15 0 h -97.5" />
                <path class="el-guide-v" d="M -112.5 0 v -291" />
                <text x="-115.5" y="-145.5" text-anchor="end">00.00m</text>
              </g>
              <rect id="<%- idPrefix %>-inputFlow" class="diag-path-ending diag-path-ending-tb" x="-2" y="-95" width="4" height="80" data-tag="inputFlow" data-dir="tb" />
              <g class="el-pump el-inputPump diag-inputPump-symbol">
                <circle r="16" cx="0" cy="0"></circle>
                <path d="M 0 -16 l 14 24 h -28 z"></path>
              </g>
            </g>
          </g>
          <path class="el-inputPump-well-outline" d="M 0 0 h 114 v 350 m 75 0 v -350 h 114" />
          <path class="el-inputPump-well-hatch" d="M 114 0 q 37.5 -15, 75 0" />
        </g>
      </g>

      <g class="al-standing" transform="translate(90,7.5) scale(0.25)">
        <path d="m 447,365 a 12.85,25 0 1 1 -0.12,0.22" transform="matrix(1.07,0.11,-0.12,0.99,-385.65,-382.09)" style="stroke-width:2.35;stroke-linecap:round" />
        <path d="m 53.1,72.03 c -11.62,8.41 -9.37,73.52 -4.29,71.01 7.61,-3.77 -6.53,-65.43 4.29,-70.5" style="stroke-width:3" />
        <path d="m 50.9,37.88 a 3.57,4.64 0 1 1 -0.03,0.04" style="stroke-width:2.05;stroke-linecap:round" />
        <path d="m 62.54,37.88 a 3.57,4.64 0 1 1 -0.03,0.04" style="stroke-width:2.05;stroke-linecap:round" />
        <path d="m 59.6,61.75 c -3.57,0 -15,0.71 -15,0.71" style="stroke-width:1.25" />
        <path d="M 48.43,77.66 C 27.54,138.05 33.79,145.33 33.79,145.34" style="stroke-width:1.9" />
        <path d="m 45.1,138.71 c 3.57,67.98 -5.76,61.68 -5.25,63.19 0.5,1.51 1.01,3.53 1.01,3.53" style="stroke-width:1.9" />
        <path d="m 48.78,139.73 c 11.68,30.18 -1.84,59.66 -1.4,62.27 0.26,1.57 13.2,1.48 13.2,1.48" style="stroke-width:1.9" />
        <path class="al-standing-limb" d="m 39.34,202.41 c -2.44,2.48 -8.16,2.48 -5.55,4.04 5.36,3.2 4.97,6.79 8.58,3.03 3.93,-4.09 13.13,-0.5 -3.03,-7.07 z" />
        <path class="al-standing-limb" d="m 52.34,199.84 c -2.44,2.48 -5.66,1.05 -3.05,2.61 5.36,3.2 12.83,6.07 16.44,2.31 3.93,-4.09 2.77,1.63 -13.38,-4.92 z" />
        <path class="al-standing-limb" d="m 37.89,145.37 c -3.48,0.17 -5.41,-8.24 -4.55,-5.33 1.78,5.99 -3.2,12.91 1.99,12.58 5.66,-0.35 10,8.51 2.55,-7.25 z" />
        <g>
          <path d="m 50.95,85.23 c 5.05,32.82 13.13,51.01 13.13,51.01" style="stroke-width:1.9" />
          <path class="al-standing-limb" d="m 66.81,136.28 c -3.48,0.17 -5.41,-8.24 -4.55,-5.33 1.78,5.99 -3.2,12.91 1.99,12.58 5.66,-0.35 10,8.51 2.55,-7.25 z" />
          <animateTransform attributeType="xml" attributeName="transform" begin="7s" dur="0.35s" type="rotate" from="0 48.95 85.23" to="-80 48.95 85.23" repeatCount="1" />
          <animateTransform id="alStandingWaveUp" attributeType="xml" attributeName="transform" begin="7.35s;alStandingWaveUpDown.end" dur="0.35s" type="rotate" from="-80 48.95 85.23" to="-110 48.95 85.23" repeatCount="1" />
          <animateTransform id="alStandingWaveUpDown" attributeType="xml" attributeName="transform" begin="alStandingWaveUp.end" dur="0.3s" type="rotate" to="-80 48.95 85.23" from="-110 48.95 85.23" repeatCount="1" />
        </g>
      </g>
    </svg>
  </div>
</div>
