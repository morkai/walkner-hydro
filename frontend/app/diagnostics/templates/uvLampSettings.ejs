<div class="form-horizontal diag-settings">
  <hr>
  <div class="control-group">
    <label class="control-label" for="settings-uvLamp-startDelay"><%- t('diagnostics', 'SETTINGS:uvLamp.startDelay') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-uvLamp-startDelay" class="span2" type="number" name="uvLamp.startDelay" required min="0" max="60" step="0.1" autofocus>
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="settings-uvLamp-stopDelay"><%- t('diagnostics', 'SETTINGS:uvLamp.stopDelay') %></label>
    <div class="controls">
      <div class="input-append">
        <input id="settings-uvLamp-stopDelay" class="span2" type="number" name="uvLamp.stopDelay" required min="0" max="60" step="0.1">
        <span class="add-on">s</span>
      </div>
    </div>
  </div>
  <hr>
</div>
