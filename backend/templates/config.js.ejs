window.appLocale = window.LOCALE = 'pl';

localStorage.setItem('LOCALE', window.appLocale);

require = {
  baseUrl: '/',
  paths: <%- paths %>,
  shim: <%- shim %>,
  config: {
    i18n: {
      locale: appLocale
    }
  }
};

if (typeof window === 'object' && location.hostname === 'hydro.walkner.pl')
{
  require.baseUrl = location.protocol
    + '//s.walkner.pl:'
    + (location.protocol === 'https:' ? 443 : 80)
    + '/hydro.walkner.pl/';
}
