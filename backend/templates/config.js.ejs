require = {
  baseUrl: '/',
  paths: <%- paths %>,
  shim: <%- shim %>,
  config: {
    i18n: {
      locale: typeof window === 'object' && window.LOCALE ? window.LOCALE : 'pl'
    }
  }
};

if (typeof window === 'object' && location.hostname === 'hydro.walkner.pl')
{
  require.baseUrl = location.protocol
    + '//s.walkner.pl:'
    + (location.protocol === 'https:' ? 443 : 80)
    + '/hydro.walkner.pl/';
}
